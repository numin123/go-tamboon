# Omise public API key
OMISE_PKEY=your_public_key
# Omise secret API key
OMISE_SKEY=your_secret_key
# Omise token endpoint URL (override if using a different environment)
OMISE_TOKEN_URL=https://vault.omise.co/tokens
# Omise charge endpoint URL (override if using a different environment)
OMISE_CHARGE_URL=https://api.omise.co/charges

# Maximum number of retry attempts for failed operations
MAX_RETRIES=5
# Maximum number of concurrent donation goroutines
MAX_DONATION_GOROUTINES=4
# Maximum number of records to process (0 means no limit)
MAX_RECORDS=10
# Number of years to increase the card expiration year for test data
EXP_YEAR_INCREASE=10
